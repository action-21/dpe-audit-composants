---
import { Audit } from 'dpe-audit'
import EtiquetteEnergieA from "../images/etiquette-energie/etiquette-energie-a.svg";
import EtiquetteEnergieB from "../images/etiquette-energie/etiquette-energie-b.svg";
import EtiquetteEnergieC from "../images/etiquette-energie/etiquette-energie-c.svg";
import EtiquetteEnergieD from "../images/etiquette-energie/etiquette-energie-d.svg";
import EtiquetteEnergieE from "../images/etiquette-energie/etiquette-energie-e.svg";
import EtiquetteEnergieF from "../images/etiquette-energie/etiquette-energie-f.svg";
import EtiquetteEnergieG from "../images/etiquette-energie/etiquette-energie-g.svg";

interface Props {
    etiquette_energie: Audit.EtiquetteEnergie;
}

const { etiquette_energie } = Astro.props;

const Icon = ((etiquette_energie) => {
    switch (etiquette_energie) {
        case Audit.EtiquetteEnergie.A:
            return EtiquetteEnergieA;
        case Audit.EtiquetteEnergie.B:
            return EtiquetteEnergieB;
        case Audit.EtiquetteEnergie.C:
            return EtiquetteEnergieC;
        case Audit.EtiquetteEnergie.D:
            return EtiquetteEnergieD;
        case Audit.EtiquetteEnergie.E:
            return EtiquetteEnergieE;
        case Audit.EtiquetteEnergie.F:
            return EtiquetteEnergieF;
        case Audit.EtiquetteEnergie.G:
            return EtiquetteEnergieG;
    }
})(etiquette_energie);
---

<Icon class="icon" />

<style>
    .icon {
        width: 100%;
        height: auto;
    }
</style>
